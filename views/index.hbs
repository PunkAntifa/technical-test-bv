<div class="header">
    <h2>Music Search</h2>
</div>    
<div class="music-form">
    <form action="/search" method="post">
        <input type="text" placeholder="Artist" name="artist">
        <input type="text" placeholder="Title" name="songTitle">
        <label for="genre">Genre</label>
        <select name="genres" id="genre">    
            {{{options data id="value" text="text"}}}
        </select>
        <button type="submit">Search</button>
    </form>
    <div class="genres-data">
        <a href="/getGenresData">Get all genres data!</a>
    </div>    
</div>